                 	;	https://sourceforge.net/projects/tinypicbootload/
                 	;
                 	;	!!!!! Set Fuse Bit CKDIV8=1 and 8MHz Calibration value !!!!!
                 	;
                 	;	Please add the following line to piccodes.ini
                 	;
                 	;	$25, 2, ATMEGA 328P, $8000, 1024, default, default,
                 	;
                 	;********************************************************************
                 
                         #define first_address max_flash-132 ; 132 word in size
                 
                 ;reg
                 #define 	crc	r20
                 #define 	cnt1	r21
                 #define 	flag	r22
                 #define 	count	r23
                 #define 	temp	r24
                 #define 	rxd	r25
                 
                 ;I/Oreg
                 #define 	UBRRL	UBRR0L
                 #define 	UCSRB	UCSR0B
                 #define 	UDR	UDR0
                 #define 	UCSRA	UCSR0A
                 
                 ;bit
                 #define 	RXEN	RXEN0
                 #define 	TXEN	TXEN0
                 #define 	UDRE	UDRE0
                 #define 	RXC	RXC0
                 
                 ;macro
                 .macro OUTI
                 		LDI	xh,high(@0)
                 		LDI	xl,low(@0)
                 		ST	x,@1
                 .endm
                 
                 .macro INI
                 		LDI	xh,high(@1)
                 		LDI	xl,low(@1)
                 		LD	@0,x
                 .endm
                 
                 .macro SBISI
                 		LDI	xh,high(@0)
                 		LDI	xl,low(@0)
                 		LD	temp,x
                 		SBRS	temp,@1
                 .endm
                 
                 .cseg
                 ;0000000000000000000000000 RESET 00000000000000000000000000
                 
                                 .org    0x0000		;;Reset vector
                 ;		RJMP	IntrareBootloader
000000 cf7f      		.dw	0xcf7f		;RJMP PC-0x80
                 
                 ;&&&&&&&&&&&&&&&&&&&&&&&   START     &&&&&&&&&&&&&&&&&
                 ;----------------------  Bootloader  ----------------------
                 ;               
                                 ;PC_flash:      C1h          AddrH  AddrL  nr  ...(DataLo DataHi)...  crc
                                 ;PIC_response:  id   K                                                 K
                 
                                 .org     first_address
                 ;               nop
                 ;               nop
                 ;               nop
                 ;               nop
                 
                                 .org     first_address+4
                 IntrareBootloader:
003f80 e993      		LDI	rxd,Calib	; set 8MHz Calibration value
003f81 e0b0
003f82 e6a6
003f83 939c      		OUTI	OSCCAL,rxd
003f84 e199      		LDI	rxd,(xtal / (16 * baud) - 1)
003f85 e0b0
003f86 eca4
003f87 939c      		OUTI	UBRRL,rxd
003f88 e198      		LDI	rxd,((1<<RXEN) | (1<<TXEN)) ; RX on,TX on
003f89 e0b0
003f8a eca1
003f8b 939c      		OUTI	UCSRB,rxd
                 
003f8c d048      		RCALL	Receive		; wait for computer
003f8d 5c91                      SUBI	rxd,0xC1	; Expect C1
003f8e f009                      BREQ	PC+2		; skip if C1
003f8f c054                      RJMP	way_to_exit	; connection errer or timeout
003f90 e295                      LDI	rxd,IdTypeAVR	; send IdType
003f91 d031              	RCALL	rs1tx
                 MainLoop:
003f92 e392      		LDI	rxd,FamilyAVR	; send ATmega Family ID
                 mainl:
003f93 d02f      		RCALL	rs1tx
003f94 2744      		CLR	crc		; clear Checksum
003f95 d03f                      RCALL	Receive		; get ADR_H
003f96 2ff9      		MOV	r31,rxd		; set r31
003f97 2f69      		MOV	flag,rxd	; set flag
003f98 bd92      		OUT	EEARH,rxd	; set EEARH
003f99 d03b                      RCALL	Receive		; get ADR_L
003f9a 2fe9      		MOV	r30,rxd		; set r30
003f9b bd91      		OUT	EEARL,rxd	; set EEARL
003f9c 0fee      		LSL	r30		; set PCPAGE:PCWORD
003f9d 1fff      		ROL	r31
003f9e 9732      		SBIW 	r30,2 		; PCPAGE:PCWORD=PCPAGE:PCWORD-2
003f9f d035                      RCALL	Receive		; get count
003fa0 2f79      		MOV	count,rxd	; set count
                 rcvoct:
003fa1 d033      	        RCALL	Receive		; get Data(L)
003fa2 2e09      		MOV	r0,rxd		; set Data(L)
003fa3 bd90      		OUT	EEDR,rxd	; set EEDR
003fa4 d030                      RCALL	Receive		; get Data(H)
003fa5 2e19      		MOV	r1,rxd		; set Data(H)
003fa6 9632      		ADIW	r30,2		; PCPAGE:PCWORD=PCPAGE:PCWORD+2
003fa7 e091      		LDI	rxd,(1<<SELFPRGEN)	; write buffer
003fa8 d014      		RCALL	ctrl_flash
003fa9 5072      		SUBI	count,2		; count=count-2
003faa f7b1                      BRNE	rcvoct		; loop
                 
003fab d029                      RCALL	Receive		; get Checksum
003fac f449      		BRNE	ziieroare	; Checksum error ?
                 
003fad fd66      		SBRC	flag,6		; is flash ?
003fae c005      		RJMP	eeprom
                 flash:
003faf e093      		LDI	rxd,((1<<PGERS)+(1<<SELFPRGEN))	; erase Flash Page
003fb0 d00c      		RCALL	ctrl_flash
003fb1 e095      		LDI	rxd,((1<<PGWRT)+(1<<SELFPRGEN))	; write Flash Page
003fb2 d00a      		RCALL	ctrl_flash
003fb3 cfde                      RJMP	MainLoop	; loop
                 eeprom:
003fb4 d003      		RCALL	w_eeprom	; write EEPROM
003fb5 cfdc      		RJMP	MainLoop	; loop
                 ziieroare:
003fb6 e49e      		LDI	rxd,'N'		; send "N"
003fb7 cfdb                      RJMP	mainl		; retry
                 
                 ; ********************************************************************
                 ;
                 ;		Write EEPROM
                 ;
                 ;		Set EEARL/EEDR and call
                 ;
                 ; ********************************************************************
                 
                 w_eeprom:
003fb8 99f9      		SBIC 	EECR,EEPE
003fb9 cffe      		RJMP	w_eeprom
003fba 9afa      		SBI	EECR,EEMPE
003fbb 9af9      		SBI	EECR,EEPE
003fbc 9508      		RET
                 
                 ; ********************************************************************
                 ;
                 ;		Write and Erace flash/buffer
                 ;
                 ;		Set R30:R31/R0:R1/rxd and call
                 ;
                 ; ********************************************************************
                 
                 ctrl_flash:
003fbd b787       		IN 	temp,SPMCSR
003fbe fd80      		SBRC 	temp,SELFPRGEN
003fbf cffd      		RJMP 	ctrl_flash
003fc0 bf97      		OUT	SPMCSR,rxd
003fc1 95e8      		SPM
003fc2 9508      		RET
                 
                 ; ********************************************************************
                 ;
                 ;		RS-232C Send 1byte
                 ;
                 ;		set rxd and call
                 ;
                 ; ********************************************************************
                 
                 rs1tx:
003fc3 e0b0
003fc4 eca0
003fc5 918c
003fc6 ff85      		SBISI	UCSRA,UDRE	; check buffer empty
003fc7 cffb      	 	RJMP	rs1tx
003fc8 e0b0
003fc9 eca6
003fca 939c      	 	OUTI	UDR,rxd		; Send 1 byte
003fcb 9508      		RET
                 
                 ; ********************************************************************
                 ;
                 ;		RS-232C Recive 1byte
                 ;
                 ;		return in rxd
                 ;
                 ; ********************************************************************
                 
                 rs1rx:
003fcc e0b0
003fcd eca0
003fce 918c
003fcf ff87      		SBISI	UCSRA,RXC	; check Recive done
003fd0 cffb      		RJMP	rs1rx		;
                 r1rx11:
003fd1 e0b0
003fd2 eca6
003fd3 919c      		INI	rxd,UDR		; Recive 1 byte
003fd4 9508      		RET
                 
                 ; ********************************************************************
                 ;
                 ;		RS-232C Recieve 1byte with Timeout and Check Sum
                 ;
                 ; ********************************************************************
                 
                 Receive:
003fd5 e151      		LDI	cnt1,xtal/500000+1	; for 20MHz => 11 => 1second
                 rpt2:
003fd6 27cc      		CLR	r28
003fd7 27dd      		CLR	r29
                 rptc:					; check Recive done
003fd8 e0b0
003fd9 eca0
003fda 918c
003fdb ff87      		SBISI	UCSRA,RXC
003fdc c003                      RJMP	PC+4
003fdd dff3      		RCALL	r1rx11		; get 1 byte
003fde 0f49                      ADD	crc,rxd		; compute checksum
003fdf 9508                      RET
                 
003fe0 9721                      SBIW	r28,1
003fe1 f7b1                      BRNE	rptc
003fe2 955a                      DEC	cnt1
003fe3 f791                      BRNE	rpt2
                 way_to_exit:
                 ;		CLR	rxd		; Disable RX,TX
                 ;		OUT	UCSRB, rxd
003fe4 cf97                      RJMP	first_address	; timeout:exit in all other cases
                 
                 ; ********************************************************************
                 ; After reset
                 ; Do not expect the memory to be zero,
                 ; Do not expect registers to be initialised like in catalog.
                 


